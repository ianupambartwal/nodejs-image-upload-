<h3>Uploaded Successful </h3>
<br>
<a class="btn btn-secondary" href="/employee">Home </a>


<h3><a class="btn btn-secondary" href="/employee"> Upload New Image </a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Images</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.imageUrl}}</td>
           
            <td>
                <a href="/employee/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/employee/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
